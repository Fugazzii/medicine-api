{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "./"
  },
  "monorepo": true,
  "root": "apps/",
  "projects": {
    "client": {
      "type": "application",
      "root": "apps/client",
      "entryFile": "main",
      "sourceRoot": "apps/client/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client/tsconfig.app.json"
      }
    },
    "doctor": {
      "type": "application",
      "root": "apps/doctor",
      "entryFile": "main",
      "sourceRoot": "apps/doctor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/doctor/tsconfig.app.json"
      }
    },
    "aws": {
      "type": "library",
      "root": "libs/aws",
      "entryFile": "index",
      "sourceRoot": "libs/aws/src",
      "compilerOptions": {
        "tsConfigPath": "libs/aws/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "client-lib": {
      "type": "library",
      "root": "libs/client-lib",
      "entryFile": "index",
      "sourceRoot": "libs/client-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/client-lib/tsconfig.lib.json"
      }
    },
    "doctor-lib": {
      "type": "library",
      "root": "libs/doctor-lib",
      "entryFile": "index",
      "sourceRoot": "libs/doctor-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/doctor-lib/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "forms-lib": {
      "type": "library",
      "root": "libs/forms-lib",
      "entryFile": "index",
      "sourceRoot": "libs/forms-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/forms-lib/tsconfig.lib.json"
      }
    },
    "facade": {
      "type": "library",
      "root": "libs/facade",
      "entryFile": "index",
      "sourceRoot": "libs/facade/src",
      "compilerOptions": {
        "tsConfigPath": "libs/facade/tsconfig.lib.json"
      }
    },
    "country-lib": {
      "type": "library",
      "root": "libs/country-lib",
      "entryFile": "index",
      "sourceRoot": "libs/country-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/country-lib/tsconfig.lib.json"
      }
    },
    "city-lib": {
      "type": "library",
      "root": "libs/city-lib",
      "entryFile": "index",
      "sourceRoot": "libs/city-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/city-lib/tsconfig.lib.json"
      }
    },
    "specialty": {
      "type": "library",
      "root": "libs/specialty",
      "entryFile": "index",
      "sourceRoot": "libs/specialty/src",
      "compilerOptions": {
        "tsConfigPath": "libs/specialty/tsconfig.lib.json"
      }
    }
  }
}